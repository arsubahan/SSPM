                </td> --}}
                {{!-- </tr>

            <tr> --}}
                {{!-- <td> --}}
                {{!-- <label>Date End:* : \{{schdet.schcompldate | date : "d MMM yyyy"}}</label>
                </td> --}}
                {{!--                 <td>
                    <input type="text" myDatePicker='' name="schcompldate" id="schcompldate"
                        datepicker-popup="d MMM yyyy" ng-model="schdet.schcompldate" datepicker-options="dateOptions"
                        size='10' ng-disabled="false" />
                </td> --}}
            </tr>
            <tr>
                <td style="vertical-align: top;">
                    <label> Description:* : \{{schdet.schdesc}}</label>
                </td>
                {{!-- <td>
                    <textarea name="schdesc" ng-model="schdet.schdesc"
                        placeholder="Please Enter Description of work to do" type="text" cols="60" rows="4">
            </textarea>

                </td> --}}
            </tr>
            {{!--             <tr>
                <td>
                    <label> Personnel : </label>
                </td>
                <td>
                    <select ng-model="schdet.schpersonnel" name='schpersonnel'
                        ng-options="opt  as opt.UsStaffname for opt in stafflist " ng-click="addperson()">
                        <option>-{{schdet.schpersonnel}}-</option>
                    </select>
            </tr> --}}
            <tr>
                <td style="vertical-align: top;">
                    <label>Staff: </label>
                    {{!-- </td>
                <td> --}}
                    <span ng-repeat="stseldet in staffselname">
                        \{{stseldet}} &nbsp;&nbsp;
